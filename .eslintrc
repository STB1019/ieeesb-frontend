{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "prettier"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style": 0
    },
    "globals": {
        "document": false
    }
}
