{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "prettier"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style": 0,
        "no-trailing-spaces": 0,
        "react/jsx-indent-props": 0,
        "max-len": 0,
        "import/no-named-as-default": 0,
        "react/prop-types": "off",
    },
    "globals": {
        "document": false
    }
}
